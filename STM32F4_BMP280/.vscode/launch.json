    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceRoot}",
            "executable": "./Build/STM32F4_BMP280.elf",
            "name": "Debug with OpenOCD",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "configFiles": [
                "interface/stlink.cfg",
                "target/stm32f4x.cfg"
            ],
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 10
            },
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none"
        },
    ]
}